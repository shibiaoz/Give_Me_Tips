{
    "manifest_version": 2,
    "name": "Show Me You are",
    "version": "1.0",
    "description": "Show Me You are",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },

   "page_action": {
      "default_icon": "images/icon19.png",
      "default_title": "Show Me You are"
   },
   "permissions": ["http://*.baidu.com/", "tabs" ],
   "web_accessible_resources": [ "page/view.html" ],
   "background": {
      "scripts": [ "lib/uri.js","js/background.js" ]
   }
}